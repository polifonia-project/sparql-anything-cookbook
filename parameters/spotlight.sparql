PREFIX fx: <http://sparql.xyz/facade-x/ns/>
PREFIX xyz: <http://sparql.xyz/facade-x/data/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX tate: <http://sparql.xyz/example/tate/>
PREFIX tsub: <http://sparql.xyz/example/tate/subject/>
PREFIX schema: <http://schema.org/>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xhtml: <http://www.w3.org/1999/xhtml#> 
PREFIX dom: <https://html.spec.whatwg.org/#>

SELECT ?resource ?surfaceForm {
	 SERVICE <x-sparql-anything:> {
		 fx:properties fx:location <http://api.dbpedia-spotlight.org/en/annotate> ;
		 	fx:http.query.text ?_text ;
			fx:http.query.confidence ?_confidence ;
			fx:media-type "application/json" ;
			fx:http.header.accept "application/json"
			.

	 [] <http://sparql.xyz/facade-x/data/%40URI> ?resource ;
	 	<http://sparql.xyz/facade-x/data/%40surfaceForm> ?surfaceForm .
	 }
}